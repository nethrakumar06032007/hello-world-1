class Calculator {
    // Method overloading
    int add(int a, int b) {
        return a + b;
    }

    double add(double a, double b) {
        return a + b;
    }
}

class AdvancedCalculator extends Calculator {
    // Method overriding
    @Override
    int add(int a, int b) {
        System.out.println("Overridden add method called");
        return a + b + 10; // adds extra 10 just for demonstration
    }
}

public class MethodDemo {
    public static void main(String[] args) {
        Calculator calc = new Calculator();
        System.out.println("Int Add: " + calc.add(5, 10));
        System.out.println("Double Add: " + calc.add(5.5, 10.5));

        AdvancedCalculator advCalc = new AdvancedCalculator();
        System.out.println("Overridden Add: " + advCalc.add(5, 10));
    }
}
